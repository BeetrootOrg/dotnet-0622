    "item": [
        {
            "name": "Get All Jira Issues Created or Updated since some time",
            "id": "043ac3a9-8dd0-4e58-af4b-9ef3c102034b",
            "request": {
                "auth": {
                    "type": "basic",
                    "basic": {
                        "username": "{{username}}",
                        "password": "{{jira-api-token}}",
                        "showPassword": false
                    }
                },
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "https://{{your-instance-name}}.atlassian.net/rest/api/2/search?jql=created>={{time-ago}}%20OR%20updated>={{time-ago}}",
                    "protocol": "https",
                    "host": [
                        "{{your-instance-name}}",
                        "atlassian",
                        "net"
                    ],
                    "path": [
                        "rest",
                        "api",
                        "2",
                        "search"
                    ],
                    "query": [
                        {
                            "key": "jql",
                            "value": "created>={{time-ago}}%20OR%20updated>={{time-ago}}"
                        }
                    ]
                }
            },
            "response": []
        },
